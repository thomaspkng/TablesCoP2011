<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.necc.tablecheck"
        versionCode="7" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>Quick Check - Tables of CoP 2011</name>

    <description>
        Quick check for the pb and pc from the Table 8.3a, 8.3b, 8.8(a), 8.8(b), 8.8(c) and 8.8(d) of CoP - Structural Use of Steel 2011
    </description>

    <author>
        TN@NECC 
    </author>

	<preference name="phonegap-version" value="2.9.0" />
	<preference name="fullscreen" value="false" />
	<preference name="orientation" value="portrait" />
	<preference name="target-device" value="universal" />
	<preference name="exit-on-suspend" value="true" />
	<preference name="android-minSdkVersion" value="7" />
	<preference name="android-installLocation" value="preferExternal" />
	<preference name="splash-screen-duration" value="3000" />

	<icon src="icon.png" />
	<gap:splash src="splash.png" />

</widget>
